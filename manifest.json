{
  "name": "Me-meme",
  "description": "Allows user to edit/memeify any image they encounter and upload it to Imgur",
  "version": "1.0",

  "background" : {
    "scripts": ["background.js"]
    },

  "content_scripts":[{
    "matches": ["http://*/", "https://*/"],
    "js":["elemFinder.js"]
    }],

  "content_security_policy":"script-src 'self' https://dme0ih8comzn4.cloudfront.net https://*.aviary.com; object-src 'self'",

  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],

  "manifest_version" : 2
}